<body>
<ul> 
<% for (let i = 0; i < item.length; i++) { %> 
<li><%= item[i] %></li> 
<% } %> 
</ul> 
<ul> 
<% item.forEach(function(item) { %> 
<li><%= item %></li> 
<% }); %> 
</ul> 

</body>
</html>